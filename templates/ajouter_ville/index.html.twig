{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}

    <style>
        .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
        .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    </style>
<div class="example-wrapper">



    <br><br>

    {{ form_start(form) }}
    <div class="w3-container w3-red">
        <h2>Input Form</h2>
    </div>
<h2>All fields marked with an asterisk (<font color="red">*</font>) are mandatory</h2>
        <label class="w3-text"><b>{{ form_label(form.ville) }}<font color="red"> *</font></b></label>
       <br> {{ form_errors(form.ville) }}
        {{ form_widget(form.ville) }}
        {{ form_help(form.ville) }}
        <br>
        <label class="w3-text"><b> {{ form_label(form.codePostal) }}<font color="red"> *</font></b></label>
        <br>{{ form_errors(form.codePostal) }}
        {{ form_widget(form.codePostal) }}
        {{ form_help(form.codePostal) }}

    


<br><br>
    {{ form_row(form.submit) }}



    {{ form_end(form) }}
</div>
{% endblock %}
